<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HotCodePush">
        <param name="android-package" value="com.nordnetab.chcp.main.HotCodePushPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="AppVersion">
        <param name="android-package" value="uk.co.whiteoctober.cordova.AppVersion" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="FileOpener2">
        <param name="android-package" value="io.github.pwlin.cordova.plugins.fileopener2.FileOpener2" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>爱生活</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <chcp>
        <config-file url="http://dsunyun.com/www/chcp.json" />
        <auto-download enabled="false" />
        <auto-install enabled="false" />
        <native-interface version="1" />
    </chcp>
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/huaqiao.png" />
    <icon density="mdpi" src="res/icon/android/huaqiao.png" />
    <icon density="hdpi" src="res/icon/android/huaqiao.png" />
    <icon density="xhdpi" src="res/icon/android/huaqiao.png" />
    <icon density="xxhdpi" src="res/icon/android/huaqiao.png" />
    <splash density="land-hdpi" src="res/icon/android/timg.jpg" />
    <splash density="land-ldpi" src="res/icon/android/timg.jpg" />
    <splash density="land-mdpi" src="res/icon/android/timg.jpg" />
    <splash density="land-xhdpi" src="res/icon/android/timg.jpg" />
    <splash density="port-hdpi" src="res/icon/android/timg.jpg" />
    <splash density="port-ldpi" src="res/icon/android/timg.jpg" />
    <splash density="port-mdpi" src="res/icon/android/timg.jpg" />
    <splash density="port-xhdpi" src="res/icon/android/timg.jpg" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="loadUrlTimeoutValue" value="60000" />
</widget>
